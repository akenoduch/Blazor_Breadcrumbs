@page "/page3"
@inject BreadcrumbService BreadcrumbService
@inject NavigationManager NavigationManager

<PageTitle>Page 3</PageTitle>

<div id="breadcrumbs">
    <Breadcrumbs GetBreadcrumbItems="BreadcrumbService.GetBreadcrumbItems"></Breadcrumbs>
</div>

<h3>Page 3</h3>

<button @onclick="NavigateToPage1">Page1</button>

@code {
    private void NavigateToPage1()
    {
        NavigationManager.NavigateTo("/page1");
    }
}
