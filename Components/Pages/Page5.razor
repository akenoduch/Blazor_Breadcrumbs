@page "/page5"
@inject BreadcrumbService BreadcrumbService
@inject NavigationManager NavigationManager

<PageTitle>Page 5</PageTitle>

<div id="breadcrumbs">
    <Breadcrumbs GetBreadcrumbItems="BreadcrumbService.GetBreadcrumbItems"></Breadcrumbs>
</div>

<h3>Page 5</h3>

<button @onclick="NavigateToPage">Page6</button>

@code {
    private void NavigateToPage()
    {
        NavigationManager.NavigateTo("/page6");
    }
}
